class Person {
    
    String name;
    int age;

    
    Person() {
        name = "Unknown";
        age = 0;
        System.out.println("No-argument constructor called!");
    }

   
    void displayInfo() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
    }
}

public class Main {
    public static void main(String[] args) {
       
        Person person = new Person();
        person.displayInfo();
    }
}
